{
  "Version": "2012-10-17",
  "Runtime": "nodejs18.x",
  "Handler": "index.handler",
  "Role": "arn:aws:iam::000000000000:role/OsinachiPortfolioGalleryRole",
  "Environment": {
    "Variables": [
      {
        "Name": "BUCKET_NAME",
        "Value": "osinachi-portfolio-gallery"
      },
      {
        "Name": "GALLERY_JSON_KEY",
        "Value": "gallery.json"
      }
    ]
  },
  "Timeout": 15,
  "MemorySize": 256
}
